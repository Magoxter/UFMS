<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bancoms.BuscarContaController">
   <children>
      <Label layoutX="37.0" layoutY="52.0" text="Editar Conta">
         <font>
            <Font name="Calibri Bold Italic" size="25.0" />
         </font>
      </Label>
      <ImageView fitHeight="97.0" fitWidth="166.0" layoutX="430.0" layoutY="24.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Java/logoBanco.png" />
         </image>
      </ImageView>
      <Label fx:id="labelConta" layoutX="167.0" layoutY="167.0" text="Conta:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="textFieldConta" layoutX="226.0" layoutY="165.0" />
      <Button fx:id="buttonBusca" layoutX="249.0" layoutY="226.0" mnemonicParsing="false" onAction="#SearchFor" prefHeight="30.0" prefWidth="104.0" text="Buscar" />
      <Label fx:id="labelErro" layoutX="247.0" layoutY="309.0" textAlignment="CENTER" textOverrun="CENTER_ELLIPSIS">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="buttonESC" layoutX="75.0" layoutY="345.0" mnemonicParsing="false" onAction="#ButtonESC" text="CANCELAR" textAlignment="CENTER" textFill="#910000" />
   </children>
</AnchorPane>
